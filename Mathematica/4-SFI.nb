    "scaleX_", ",", "scaleY_", ",", "rotateX_", ",", " ", "rotateY_", ",", 
     "translateX_", ",", "translateY_"}], "]"}], "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Cos", "[", "rotateX", "]"}], ",", 
      RowBox[{"Sin", "[", "rotateX", "]"}]}], "}"}], "*", "scaleX", "*", 
    "x"}], "+", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Sin", "[", "rotateY", "]"}]}], ",", 
      RowBox[{"Cos", "[", "rotateY", "]"}]}], "}"}], "*", "scaleY", "*", 
    "y"}], "+", 
   RowBox[{"{", 
    RowBox[{"translateX", ",", "translateY"}], "}"}]}]}]}], "Input",
